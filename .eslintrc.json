{
  "env": {
    "browser": true,
    "es2021": true
    },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-restricted-syntax": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "import/no-dynamic-require": "off",
    "import/no-unresolved": "off",
    "no-useless-catch": "off",
    "global-require": "off",
    "guard-for-in": "off",
    "no-use-before-define": ["off", { "classes": false }]
  },
  "ignorePatterns": [
    "*.json",
    "*.d.ts",
    "*.html"
  ]
}
